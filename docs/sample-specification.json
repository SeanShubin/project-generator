{
  "prefix": [
    "com",
    "seanshubin"
  ],
  "name": [
    "kotlin",
    "tryme"
  ],
  "description": "Kotlin experiments",
  "version": "0.1.0",
  "language": "kotlin",
  "developer": {
    "name": "Sean Shubin",
    "githubName": "SeanShubin",
    "mavenUserName": "SeanShubin",
    "organization": "Sean Shubin",
    "url": "http://seanshubin.com/"
  },
  "global": [
    "stdlib",
    "test"
  ],
  "dependencies": {
    "stdlib": {
      "group": "org.jetbrains.kotlin",
      "artifact": "kotlin-stdlib-jdk8"
    },
    "coroutines": {
      "group": "org.jetbrains.kotlinx",
      "artifact": "kotlinx-coroutines-core"
    },
    "test": {
      "group": "org.jetbrains.kotlin",
      "artifact": "kotlin-test-junit",
      "scope": "test"
    },
    "arrow": {
      "group": "io.arrow-kt",
      "artifact": "arrow-core"
    },
    "jackson": {
      "group": "com.fasterxml.jackson.module",
      "artifact": "jackson-module-kotlin"
    },
    "jackson-time": {
      "group": "com.fasterxml.jackson.datatype",
      "artifact": "jackson-datatype-jsr310"
    }
  },
  "modules": {
    "domain": [
      "coroutines",
      "jackson",
      "jackson-time"
    ],
    "console": [
      "domain"
    ]
  },
  "entryPoints": {
    "console": "com.seanshubin.kotlin.tryme.console.MainKt"
  },
  "javaVersion": "25",
  "sourceDependencies": [
    {
      "sourceProjectPath": "../another-project",
      "moduleMapping": {
        "source-module-name": "target-module-name"
      }
    }
  ],
  "exports": [
    "domain"
  ]
}
